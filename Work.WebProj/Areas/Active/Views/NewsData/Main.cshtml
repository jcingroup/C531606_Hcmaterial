@{
    Layout = "~/Views/Shared/Manage/RLayout.cshtml";
}
@section breadCrumb {
    <li><a href="~/Active/Product/Product"><i class="fa-list-alt"></i> @ViewBag.MenuName</a></li>
    <li>@ViewBag.Caption</li>
}
@section IncludeCSS{
    <link rel="stylesheet" href="~/node_modules/react-datepicker/dist/react-datepicker.css">
}
@section IncludeScript{
    @Html.Partial("Manage/JSGlobalVar")
    <script>
        var gb_menuname = '@ViewBag.MenuName';
        var gb_caption = '@ViewBag.Caption';
    </script>
    <script src="~/ckeditor/ckeditor.js?v=@CommWebSetup.CacheVer"></script>
    <script src="~/Scripts/build/app/m_news.js?v=@CommWebSetup.CacheVer"></script>
}
@section sidebar{
    @Html.Partial("Manage/vucMenu")
}